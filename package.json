{
  "name": "@mumen/try-catch-alt",
  "version": "1.0.0",
  "description": "An alternative library for try-catch blocks, helps reduce the code and make it cleaner and more readable using anonymous functions.",
  "main": "main.js",
  "types": "main.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MumenTayyem/try-catch-alt.git"
  },
  "files": [
    "/*"
  ],
  "scripts": {
    "build": "tsc --declaration && cp package.json output/package.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "pack-locally": "cd output && npm pack --pack-destination /Users/mumentayyem/personal/tools",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "try-catch",
    "try",
    "catch",
    "error",
    "handling"
  ],
  "author": "Mumen Tayyem",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.3.3"
  }
}